{
	"name" : "bordercloud/sparql",
	"type" : "library",
	"description" : "Lib PHP very easy for SPARQL 1.1",
	"keywords" : [
		"SPARQL",
		"RDF"
	],
	"homepage" : "https://github.com/BorderCloud/SPARQL",
	"license" : "CC-BY-SA-4.0",
	"version" : "2.2.0",
	"authors" : [{
			"name" : "Karima Rafes",
			"email" : "karima.rafes@gmail.com",
			"homepage" : "https://github.com/BorderCloud/SPARQL",
			"role" : "Developer"
		}
	],
	"require" : {
		"php" : ">=8.1",
    "ext-xml": "*",
    "ext-json": "*",
    "ext-curl": "*"
  },
	"repositories" : [{
			"type" : "vcs",
			"url" : "https://github.com/BorderCloud/SPARQL"
		}
	],
	"autoload" : {
		"psr-4" : {
			"BorderCloud\\SPARQL\\" : "src/",
      "BorderCloud\\SPARQL\\Tests\\" : "tests/"
		}
	},
  "bin" : [
    "bin/query"
  ],
  "config" : {
    "bin-dir" : "bin"
  },
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"clean/phpdoc-md": "^0.19.1"
	}
}
